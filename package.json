{
  "name": "tauque",
  "version": "1.3.2",
  "description": "Tauque is a zero-configuration JS/TS bundler with serious pulling power. It's up to 100x quicker than Rollup/Webpack with Babel.",
  "homepage": "https://github.com/CTNicholas/tauque#readme",
  "repository": "github:CTNicholas/tauque",
  "bugs": {
    "url": "https://github.com/CTNicholas/tauque/issues"
  },
  "bin": "bin/tauque.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "esbuild src/index.js --bundle --minify --format=cjs --platform=node --target=node8 --outfile=dist/esbundle.js",
    "dev": "nodemon --watch src --exec \"npm run build\"",
    "tauquedev": "tauque --dev",
    "tauque": "tauque"
  },
  "author": "CTNicholas",
  "license": "MIT",
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "ansi-escapes": "^4.3.1",
    "esbuild": "^0.12.19",
    "node-watch": "^0.7.0"
  },
  "keywords": [
    "tauque",
    "bundler",
    "bundle",
    "package",
    "esbuild",
    "rollup",
    "webpack",
    "babel",
    "zero-config",
    "easy",
    "typescript",
    "jsx",
    "css"
  ]
}
